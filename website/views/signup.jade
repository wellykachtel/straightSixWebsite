extends layout

block content
	div.row
		div.col-xs-4
		div.col-xs-4
			div.form-group
				label(for='first_name') First Name
				input(type="text" class="form-control" id="first_name")
			div.form-group
				label(for='last_name') Last Name
				input(type="text" class="form-control" id="last_name")
			div.form-group
				label(for='email') Email
				input(type="email" class="form-control" id="email")
			div.form-group
				label(for='cellphone') Cellphone
				input(type="text" class="form-control" id="cellphone")
			div.form-group
					label(for='level') Skill Level
						a(href="/about/levels")
							span.glyphicon.glyphicon-question-sign
					select.form-control#level(name="level")
						option AA
						option A
						option BB
						option B
			div.form-group
				label(for="comments") Additional Comments
				textarea(class="form-control" rows="3" id="comments")
			button(type='submit' class="btn btn-info btn-block") Submit
